# local template 60 : adding refdate and reftime

unsigned[1] centuryOfAnalysis = 0 :dump;
unsigned[1] yearOfAnalysis = 0 : dump;
unsigned[1] monthOfAnalysis = 0 : dump;
unsigned[1] dayOfAnalysis = 0 : dump;
unsigned[1] hourOfAnalysis = 0 : dump;
unsigned[1] minuteOfAnalysis = 0 : dump;
unsigned[1] secondsOfAnalysis = 0 : dump;

meta dateOfAnalysis  g1date(centuryOfAnalysis,yearOfAnalysis,monthOfAnalysis,dayOfAnalysis) : dump;
meta timeOfAnalysis  time(hourOfAnalysis,minuteOfAnalysis,secondsOfAnalysis) : dump;

#alias mars.refdate = dateOfAnalysis;
#alias mars.reftime = timeOfAnalysis;
alias mars.andate = dateOfAnalysis;
alias mars.antime = timeOfAnalysis;
